			<section>
				<div class="p-lower__section p-lower__section--search">
					<div class="l-container">
						<div class="c-pagination__information c-pagination__information--top">
							<div class="c-pagination__informationLabel">検索結果</div>
							<div class="c-pagination__informationCount">読み込み中...</div>
						</div>
						<ol class="p-search__list">
							<li class="p-search__item p-search__item--dog">
								<a class="p-search__link" href="#">
									<div class="p-search__image">
										<img src="https://vdev.impactm.net/static/images/temp/temp_img_qa.jpg" alt="テスト" class="">
									</div>
									<p class="p-search__title">読み込み中...</p>
								</a>
								<a class="p-search__tag" href="#">
									お悩み
								</a>
								<!-- <time class="p-search__date" datetime="">2021.02.08</time> -->
							</li>
						</ol>

						<div class="c-pagination">
							<div class="c-pagination__information c-pagination__information--bottom">
								<div class="c-pagination__informationLabel">検索結果</div>
								<div class="c-pagination__informationCount">読み込み中...</div>
							</div>
							<ul class="c-pagination__list c-pagination__list--bottom">
								<li class="c-pagination__item c-pagination__item--prev">
									<a href="{{prevUrl}}" {{#firstPage}}is-disabled{{/firstPage}}>←前へ</a>
								</li>
								<li class="c-pagination__item c-pagination__item--page">
									<ul>
	<!--
										{{#pages}}
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number {{#currentPage}}is-current{{/currentPage}}"><a href="{{url}}">{{page}}</a></li>
										{{/pages}}
	-->
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number is-currentPage"><a href="{{url}}">1</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">2</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">3</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">4</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">5</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">6</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">7</a></li>
										<li class="c-pagination__item c-pagination__item--number js-pagination__item--number"><a href="{{url}}">8</a></li>
